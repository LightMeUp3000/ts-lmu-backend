{
  "name": "ts-lmu-backend",
  "version": "1.0.0",
  "description": "",
  "author": "Jakub Lewek",
  "main": "index.js",
  "scripts": {
    "start": "ts-node src/index.ts",
    "lint": "tslint --quiet './src/**/*.ts'",
    "lint:fix": "tslint --fix './src/**/*.ts'",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "cluster": "ts-node clusters.ts"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "ts-node": "^9.0.0"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.2",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "nodemon": "^2.0.4",
    "prettier": "^2.2.1",
    "supertest": "^5.0.0",
    "ts-jest": "^26.4.1",
    "tslint": "^6.0.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^9.0.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "^4.0.3"
  }
}
